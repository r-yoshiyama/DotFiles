{
	"templete": {
		"prefix": "templete",
		"body": [
			"precision mediump float;",
			"uniform float time; // time",
			"uniform vec2 resolution; // resolution",
			"",
			"void main(void) {",
			"\tvec2 position = (gl_FragCoord.xy * 2.0 - resolution) / min(resolution.x, resolution.y);",
			"",
			"\t$0",
			"",
			"\tgl_FragColor = vec4(0.0, 0.0, 0.0, 1.0);",
			"}"
		],
		"description": "templete"
	},
}