{
	"Templete": {
		"prefix": "templete",
		"body": [
			"\\documentclass{jsarticle}[a4j,11pt]",
			"\\usepackage[dvipdfmx]{graphicx}",
			"\\usepackage[dvipdfmx]{color}",
			"\\usepackage{listings,plistings}",
			"\\usepackage{url}",
			"\\usepackage{here}",
			"\\usepackage{amsmath}",
			"\\usepackage{amssymb}",
			"\\usepackage{amsfonts}",
			"\\usepackage{caption}",
			"\\usepackage{algorithm, algpseudocode}",
			"\\usepackage{multicol}",
			"\\usepackage[dvipdfmx, bookmarkstype=toc, colorlinks=false, pdfborder={0 0 0}, bookmarks=true, bookmarksnumbered=true]{hyperref}",
			"\\usepackage{pxjahyper}",
			"\n\n",
			"\\begin{document}",
			"$0",
			"\\end{document}"
		],
		"description": "Define Templete"
	},
	"Title Page": {
		"prefix": "title",
		"body": [
			"\\begin{titlepage}",
			"\t\\begin{flushright}",
			// "\t\t{\\large",
			// "\t\t指導教員（主査）：ooooo\\ 教授 \\\\",
			// "\t\t副査：xxxxxx\\ 教授",
			// "\t\t}",
			// "\t\\end{flushright}",
			"\t\\begin{center}",
			"\t\t\\vspace{12mm}",
			"\t\t{\\Huge ${1:Title}}\\\\",
			"\t\t\\vspace{20mm}",
			"\t\t{\\Huge ${2:SubTitle}}\\\\",
			"\t\t\\vspace{100mm}",
			"\t\t{\\huge {3:Part}}\\\\",
			"\t\t\\vspace{5mm}",
			"\t\t{\\huge 氏名\\ \\ \\ \\ $1\\\\",
			"\t\t\\vspace{5mm}",
			"\t\t{\\huge ${4:Member}}\\",
			"\t\t\\vspace{5mm}",
			"\t\t{\\huge 日付\\,\\,\\today}",
			"\t\\end{center}",
			"\\end{titlepage}",
			"\n",
			"$0"
		],
		"description": "Title Page Templetes"
	},
	"Image": {
		"prefix": "image",
		"body": [
			"\\begin{figure}[H]",
			"\\centering",
			"\\includegraphics[width = 15cm]{${1:dir}}",
			"\\caption{${2:Title}}",
			"\\label{fig:${3:Label}}",
			"\\end{figure}"
		]
	},
	"Table_of_Contents": {
		"prefix": "toc",
		"body": [
			"\\setcounter{tocdepth}{4}",
			"\\tableofcontents",
			"\\clearpage",
			"\\listoffigures",
			"\\listoftables",
			"\\clearpage"
		]
	},
	"Listings": {
		"prefix": "listing",
		"body": [
			"\\lstinputlisting[basicstyle=\\ttfamily\\footnotesize, frame=single,breaklines = true,numbers=left, caption=${1:title},label=${2:label}]{${3:filename}}"
		]
	},
	"List": {
		"prefix": "list",
		"body": [
			"\\begin{itemize}",
			"%\\begin{enumerate} %番号付き箇条書き",
			"%\\begin{description} %タイトル付き箇条書き \\item [title] text",
			"\\setlength{\\itemsep}{-0.5mm}",
			"\\setlength{\\parskip}{-0.5mm}",
			"\\item ${1:item}",
			"\\end{itemize}"
		]
	},
	"Table": {
		"prefix": "table",
		"body": [
			"\\begin{table}[H]",
			"%\\centering",
			"\\begin{center}",
			"\\caption{${1:Title}}",
			"%\\scalebox{0.9}[0.9]{",
			"\\begin{tabular}{|l|c|r|}\\hline",
			"\t${2:aaa} & ${3:bbb} & ${4:ccc} \\\\ \\hline \\hline",
			"\t%\\multicolumn {tab_num}{|l|}{} \\\\ \\hline \\hline % マルチカラム",
			"\t${5:ddd} & ${6:eee} & ${7:fff}\\\\ \\hline",
			"\\end{tabular}",
			"% }",
			"\\end{center}",
			"\\label{tb:${8:label}}",
			"\\end{table}"
		]
	},
	"Bibliography": {
		"prefix": "bib",
		"body": [
			"\\begin{thebibliography}{9}",
			"%\\addcontentsline{toc}{section}{\\refname}",
			"\t\\bibitem{${1:label}} ${2:title_url}",
			"\\end{thebibliography}"
		],
		"description": "Bibliography"
	}
}